<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="css/results.css">
    <title>Results</title>
  </head>
  <body>
    <div class="row abcc">
      <img src="/images/butt.png" alt="">
      </div>
   
      <div class="container">
          <div class="row">
            <% for(var i=0; i!=results.length;i++){ %>
                <div class="col-lg-1 ">
               
                </div> 
                <div class="col-lg-4 res">

                        <img src="<%= results[i].image.url %>" alt="" >
                        <h1 class="display-4"><%= results[i].name %></h1>
                        <span class="spin"><%= results[i].powerstats.intelligence %></span>
                        <span class="spst"><%= results[i].powerstats.strength %></span>
                        <span class="sppo"><%= results[i].powerstats.power %></span>
                        <span class="spsp"><%= results[i].powerstats.speed %></span><br>

                        <div class="spin1"><p>Intelligence: <%= results[i].powerstats.intelligence %> </p></div>
                       
                        <div class="spst1"><p>Strength: <%= results[i].powerstats.strength %></p></div>
                
                        <div class="spsp1"><p>Speed: <%= results[i].powerstats.speed %></p></div>
                       
                        <br>


              </div>
                <div class="col-lg-1 ">
                    
                </div> 
            <% } %> 
          </div>

      </div>

  </body>
    <script src="js/results.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
</html>



